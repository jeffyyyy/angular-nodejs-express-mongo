<div ng-include src="'/public/partials/header.ejs'"></div>
<div class="container">
	<div class="well">
		<h2>Welcome to Jeff's Experiment</h2>
		<ul id="messages">
			<li ng-repeat="msg in messages track by $index">{{msg}}</li>
		</ul>
		<form class="form chat">
			<div class="form-group ">
				<input id="message" class="form-control" ng-model="message" autocomplete="off" />
			</div>
			<button class="btn btn-default" ng-click="chat(message, $event)">Send</button>
		</form>
	</div>
</div>

<div ng-include src="'/public/partials/footer.ejs'"></div>
